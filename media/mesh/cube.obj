#
# Modelo de cube de prueba ...
#
mtllib cube.mtl
g main
# front vertex
v -0.5 -0.5 0.5
v 0.5 -0.5 0.5
v 0.5 0.5 0.5
v -0.5 0.5 0.5
# back vertex
v -0.5 -0.5 -0.5
v 0.5 -0.5 -0.5
v 0.5 0.5 -0.5
v -0.5 0.5 -0.5
vt 0 0 0
vt 1 0 0
vt 1 1 0
vt 0 1 0
vn 0 0 1
vn 0 0 -1
vn 0 1 0
vn 0 -1 0
vn 1 0 0
vn -1 0 0
usemtl cube
# front face
f 1/1/1 2/2/1 3/3/1
f 3/3/1 4/4/1 1/1/1
# back face
f 6/1/2 5/2/2 8/3/2
f 8/3/2 7/4/2 6/1/2
# top face
f 4/1/3 3/2/3 7/3/3
f 7/3/3 8/4/3 4/1/3
# bottom face
f 5/1/4 6/2/4 2/3/4
f 2/3/4 1/4/4 5/1/4
# right face
f 2/1/5 6/2/5 7/3/5
f 7/3/5 3/4/5 2/1/5
# left face
f 5/1/6 1/2/6 4/3/6
f 4/3/6 8/4/6 5/1/6
